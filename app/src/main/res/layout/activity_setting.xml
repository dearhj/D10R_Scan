                android:text="@string/save_config_to_file"
                android:textColor="@color/black_333333"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@drawable/baseline_arrow_forward_ios_24" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/overtime"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@drawable/btn_public_nobgtoqbreakbg_onclickstyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/time_out"
                android:textColor="@color/black_333333"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@drawable/baseline_arrow_forward_ios_24" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/development"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@drawable/btn_public_nobgtoqbreakbg_onclickstyle">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/developer"
                android:textColor="@color/black_333333"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_development_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="3dp"
                android:textColor="@color/black_333333"
                android:textSize="12sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:src="@drawable/baseline_arrow_forward_ios_24" />
        </RelativeLayout>
    </LinearLayout>
</ScrollView>